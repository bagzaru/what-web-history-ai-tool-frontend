{
    "manifest_version": 3,
    "name": "testExt",
    "description": "this is my first extension",
    "version": "1.0",
    "icons": {
      "256": "icon.png"
    },
    "background": {
      "service_worker": "scripts/service-worker.js",
      "type": "module"
    },
    "action": {
      "default_icon": "icon.png",
      "default_title": "KJS",
      "default_popup": "popup/main.html",
      "default_width": 200,
      "default_height": 100
    },
    "permissions":[
      "activeTab", 
      "tabs",
      "scripting",
      "notifications",
      "storage",
      "webNavigation",
      "webRequest",
      "windows",
      "identity"
    ],
    "oauth2": {
      "client_id": "304314144761-9gfb9l023pe0cpi5p9efn21uonp9nsfo.apps.googleusercontent.com",
      "scopes" : ["profile", "email"]
    },

    "content_scripts":[
      {
        "matches": ["https://*/*", "http://*/*"],
        "js": ["scripts/content-scripts/domReadyHandler.js", "scripts/content-scripts/domDistiller.js"],
        "type": "module"
      }
    ],
    
    "host_permissions": ["*://*/*"],
    "key": ""
  }